{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "build": "tsc",
    "serve": "npm run local-predeploy && npm run build && firebase emulators:start --import=exportedlocal",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "use-dev": "firebase use dev",
    "deploy": "firebase deploy --only functions",
    "local-predeploy": " cp ../dist/index.html templates/index.html && cp ../src/config/project.ts ./src/common/project.ts",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@sentry/node": "^6.19.7",
    "ethereumjs-util": "^7.1.4",
    "ethers": "^5.6.9",
    "express": "^4.18.1",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.19.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.34",
    "osm-static-maps": "^3.10.3",
    "puppeteer": "^14.1.0",
    "sharp": "^0.30.4",
    "uuid-v4": "^0.1.0",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-plugin-import": "^2.26.0",
    "firebase-functions-test": "^0.3.3",
    "firebase-tools": "^10.9.2",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "private": true
}
